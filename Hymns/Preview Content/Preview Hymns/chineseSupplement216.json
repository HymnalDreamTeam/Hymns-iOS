{
    "lyrics": [
        {
            "transliteration": [
                "hē ！ cóng bǎo zuò líu chū",
                "chún jìng shēng mìng hé de shǔi ！",
                "chī ！ shēng mìng shù guǒ zǐ ，",
                "fēng shuò yòu léi léi chúi chúi ！",
                "kàn ！ nà mǎn chéng guāng hūi ，",
                "bù xū dēng guāng huò rì 、 yuè ，",
                "tā méi yǒu hēi yè ！"
            ],
            "verse_content": [
                "喝！從寶座流出",
                "純淨生命河的水！",
                "喫！生命樹果子，",
                "豐碩又纍纍垂垂！",
                "看！那滿城光輝，",
                "不需燈光或日、月，",
                "她沒有黑夜！"
            ],
            "verse_type": "verse"
        },
        {
            "transliteration": [
                "lái bà ！ é ， lái bà ！",
                "shèng líng yǔ xīn fù shuō ；",
                "lái bà ！ é ， lái bà ！",
                "tīng jiàn de rén yě shuō ；",
                "lái bà ！ é ， lái bà ！",
                "kǒu kě yòu yuàn yì de",
                "kě bái bái lái hē ！"
            ],
            "verse_content": [
                "來罷！哦，來罷！",
                "聖靈與新婦說；",
                "來罷！哦，來罷！",
                "聽見的人也說；",
                "來罷！哦，來罷！",
                "口渴又願意的",
                "可白白來喝！"
            ],
            "verse_type": "chorus"
        },
        {
            "transliteration": [
                "jī dū nǎi shì wǒ men hé shǔi ，",
                "zài lǐ miàn yǒng líu ；",
                "jī dū nǎi shì shēng mìng shù guǒ ，",
                "gōng wǒ men xiǎng shòu ；",
                "jī dū nǎi shì wǒ men de guāng 、",
                "chén xīng yǔ bái zhòu ；",
                "tā nǎi shì wàn yǒu ！"
            ],
            "verse_content": [
                "基督乃是我們河水，",
                "在裡面湧流；",
                "基督乃是生命樹果，",
                "供我們享受；",
                "基督乃是我們的光、",
                "晨星與白晝；",
                "祂乃是萬有！"
            ],
            "verse_type": "verse"
        },
        {
            "transliteration": [
                "wǒ men xǐ jìng páo zǐ ，",
                "yǒu quán lái chī shēng mìng shù ，",
                "『 é zhǔ ， ā men ，",
                "ā lì lù yà ， 』",
                "wǒ men zhēn yǒu fú ！",
                "wǒ men cāo liàn wǒ men de líng ，",
                "lái jīng lì jī dū ─",
                "hé děng de jī dū ！"
            ],
            "verse_content": [
                "我們洗淨袍子，",
                "有權來喫生命樹，",
                "『哦主，阿們，",
                "阿利路亞，』",
                "我們真有福！",
                "我們操練我們的靈，",
                "來經歷基督─",
                "何等的基督！"
            ],
            "verse_type": "verse"
        },
        {
            "transliteration": [
                "xiàn zài wǒ men yǒu yī gè jiā ，",
                "hūi huáng shèng tài yáng ，",
                "dì xiōng zǐ mèi tóng xīn zhàn zhù",
                "hé yī de lì cháng ；",
                "dì fāng zhào hùi nǎi shì",
                "xīn yé lù sā lěng yù cháng ─",
                "hé děng de yù cháng ！"
            ],
            "verse_content": [
                "現在我們有一個家，",
                "輝煌勝太陽，",
                "弟兄姊妹同心站住",
                "合一的立場；",
                "地方召會乃是",
                "新耶路撒冷豫嘗─",
                "何等的豫嘗！"
            ],
            "verse_type": "verse"
        }
    ],
